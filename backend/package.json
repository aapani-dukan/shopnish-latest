{
      "name": "shopnish-backend",
      "version": "1.0.0",
      "description": "",
      "main": "dist/server/index.js",
      "type": "module",
      "scripts": {
            "build": "esbuild server/index.ts --bundle --platform=node --format=esm --outdir=dist/server --alias:shared=./shared --packages=external --external:compression --external:sirv && cp -r server/migrations dist/server/migrations && cp -r shared dist/shared",
            "start": "cross-env NODE_ENV=production node -r tsconfig-paths/register dist/server/index.js",
            "dev": "nodemon --watch server --ext ts,tsx --exec tsx server/index.ts",
            "seed": "tsx server/seed.ts",
            "db:push": "drizzle-kit push",
            "db:push:force": "drizzle-kit push --force",
            "db:migrate": "drizzle-kit migrate"
      },
